{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "v1.1.2",
        "box/spout": "2.8.x-dev",
        "mikey179/vfsstream": "2.0.x-dev",
        "phpoffice/phpspreadsheet": "1.3.1",
        "phpunit/dbunit": "3.0.3",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/annotations-pack": "v1.0.0",
        "symfony/apache-pack": "v1.0.1",
        "symfony/asset": "4.1.x-dev",
        "symfony/browser-kit": "4.1.x-dev",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "4.1.x-dev",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/maker-bundle": "1.0.0",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "v1.0.3",
        "symfony/twig-bundle": "4.0.x-dev",
        "symfony/validator": "4.1.x-dev",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "symfony/dotenv": "^4.1",
        "symfony/webpack-encore-bundle": "v1.1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
